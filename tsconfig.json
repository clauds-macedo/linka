{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@ui/*": [
        "src/ui/*"
      ],
      "@features/*": [
        "src/features/*"
      ],
      "@domain/*": [
        "src/domain/*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx"
  ]
}
